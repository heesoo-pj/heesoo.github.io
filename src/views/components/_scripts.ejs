<% if (typeof page.scripts !== 'undefined' && page.scripts.desktop instanceof Array) { %>
  <% page.scripts.desktop.forEach(function(desktopJsPath) { %>
    <script src="<%= desktopJsPath %>"></script>
  <% }) %>
<% } %>

<% if (typeof page.scripts !== 'undefined' && page.scripts.common instanceof Array) { %>
  <script src="/scripts/components/store/layerTransform.js"></script>
  <% page.scripts.common.forEach(function(commonJsPath) { %>
    <script src="<%= commonJsPath %>"></script>
  <% }) %>
<% } %>

<% if (!page.metaPage || (typeof page.metaPage !== 'undefined' && page.metaPage.header)) { %>
  <script src="/scripts/common/header.js"></script>
<% } %>
